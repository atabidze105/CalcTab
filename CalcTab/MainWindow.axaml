			HorizontalAlignment="Stretch"
			VerticalAlignment="Stretch"/>
		<Button
			Name="But_tanh"
			Classes="Button4"
			Click="function"
			Content="tanh"
			Grid.Row="6"
			Grid.Column="6"
			HorizontalContentAlignment="Center"
			VerticalContentAlignment="Center"
			HorizontalAlignment="Stretch"
			VerticalAlignment="Stretch"/>
		<!--5 ряд-->
		<Button
			Name="But_1"
			Classes="Button2"
			Click="inputNumber"
			Content="1"
			Grid.Row="7"
			Grid.Column="0"
			Background="#ECEDEE"
			HorizontalContentAlignment="Center"
			VerticalContentAlignment="Center"
			HorizontalAlignment="Stretch"
			VerticalAlignment="Stretch"/>
		<Button
			Name="But_2"
			Classes="Button2"
			Click="inputNumber"
			Content="2"
			Grid.Row="7"
			Grid.Column="1"
			Background="#ECEDEE"
			HorizontalContentAlignment="Center"
			VerticalContentAlignment="Center"
			HorizontalAlignment="Stretch"
			VerticalAlignment="Stretch"/>
		<Button
			Name="But_3"
			Classes="Button2"
			Click="inputNumber"
			Content="3"
			Grid.Row="7"
			Grid.Column="2"
			Background="#ECEDEE"
			HorizontalContentAlignment="Center"
			VerticalContentAlignment="Center"
			HorizontalAlignment="Stretch"
			VerticalAlignment="Stretch"/>
		<Button
			Name="But_plus"
			Classes="Button1"
			Click="operation"
			Content="+"
			Grid.Row="7"
			Grid.Column="3"
			HorizontalContentAlignment="Center"
			VerticalContentAlignment="Center"
			HorizontalAlignment="Stretch"
			VerticalAlignment="Stretch"/>
		<Button
			Name="But_log"
			Classes="Button4"
			Click="function"
			Content="ln"
			Grid.Row="7"
			Grid.Column="4"
			HorizontalContentAlignment="Center"
			VerticalContentAlignment="Center"
			HorizontalAlignment="Stretch"
			VerticalAlignment="Stretch"/>
		<Button
			Name="But_ln"
			Classes="Button4"
			Click="function"
			Content="log"
			Grid.Row="7"
			Grid.Column="6"
			HorizontalContentAlignment="Center"
			VerticalContentAlignment="Center"
			HorizontalAlignment="Stretch"
			VerticalAlignment="Stretch"/>
		<!--6 ряд-->
		<Button
			Name="But_PlsMns"
			Classes="Button2"
			Click="symChange"
			Content="&#177;"
			Grid.Row="8"
			Grid.Column="0"
			Background="#ECEDEE"
			HorizontalContentAlignment="Center"
			VerticalContentAlignment="Center"
			HorizontalAlignment="Stretch"
			VerticalAlignment="Stretch"/>
		<Button
			Name="But_0"
			Classes="Button2"
			Click="inputNumber"
			Content="0"
			Grid.Row="8"
			Grid.Column="1"
			Background="#ECEDEE"
			HorizontalContentAlignment="Center"
			VerticalContentAlignment="Center"
			HorizontalAlignment="Stretch"
			VerticalAlignment="Stretch"/>
		<Button
			Name="But_fr"
			Classes="Button2"
			Click="inputOther"
			Content=","
			Grid.Row="8"
			Grid.Column="2"
			Background="#ECEDEE"
			HorizontalContentAlignment="Center"
			VerticalContentAlignment="Center"
			HorizontalAlignment="Stretch"
			VerticalAlignment="Stretch"/>
		<Button
			Name="But_equal"
			Classes="Button3"
			Content="="
			Click="operation"
			Background="#1a73e8"
			Foreground="White"
			Grid.Row="8"
			Grid.Column="3"
			FontSize="30"
			HorizontalContentAlignment="Center"
			VerticalContentAlignment="Center"
			HorizontalAlignment="Stretch"
			VerticalAlignment="Stretch"/>
		<Button
			Name="But_fact"
			Classes="Button4"
			Click="function"
			Content="&#119909;!"
			Grid.Row="8"
			Grid.Column="4"
			HorizontalContentAlignment="Center"
			VerticalContentAlignment="Center"
			HorizontalAlignment="Stretch"
			VerticalAlignment="Stretch"/>
	</Grid>
</Window>
